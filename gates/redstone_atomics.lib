library(redstone) {
  time_unit: 100ms;
  cell(repeater) {
    area : 2;
    pin(A) {
      direction : input;
    }
    pin(Y) {
      direction : output;
      function : "A";
    }
  }  

  cell(torch) {
    area: 2
    pin(A) {
      direction: input;
    }
    pin(Y) {
      function: "A'";
      direction: output;
      timing() {
        timing_type : rising_edge;
        intrinsic_rise : 1;
        intrinsic_fall : 1;
        rise_resistance : 0;
        fall_resistance : 0;
        related_pin : "A";
      }
    }
  }

  cell(latch) {
    area : 4;
    latch ("IQ","IQN") {
      enable : "G";
      data_in : "D";
    }

    pin(D) {
      direction : input;
    }
    pin(G) {
      direction : input;
    }
 
    pin(Q) {
      direction : output;
      function : "IQ";
      internal_node : "Q";
      
      timing() {
        timing_type : rising_edge;
        intrinsic_rise : 1;
        intrinsic_fall : 1;
        rise_resistance : 0;
        fall_resistance : 0;
        related_pin : "G";
      }
      
      timing() {
        timing_sense : positive_unate;
        intrinsic_rise : 1;
        intrinsic_fall : 1;
        rise_resistance : 0;
        fall_resistance : 0;
        related_pin : "D";
      }
    }
  }
}
